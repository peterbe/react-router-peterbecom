{
  "name": "react-router-peterbecom",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build",
    "dev": "react-router dev",
    "start": "cross-env NODE_ENV=production node --experimental-strip-types --no-warnings start-server.ts ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "biome check server app headless tests scripts vite.config.ts server.ts start-server.ts mock-peterbecom-backend",
    "lint:fix": "biome check --write server app headless tests scripts vite.config.ts server.ts start-server.ts mock-peterbecom-backend",
    "prepare": "husky",
    "test": "BUILD_PATH=./build/server/index.js vitest --run",
    "build:compress": "node --experimental-strip-types --no-warnings scripts/compress-build.ts",
    "compare-build-dirs": "node --experimental-strip-types --no-warnings scripts/compare-build-dirs.ts",
    "test:watch": "BUILD_PATH=./build/server/index.js vitest",
    "mock-peterbecom-backend": "tsx mock-peterbecom-backend/index.ts",
    "mock-peterbecom-backend:watch": "tsx watch mock-peterbecom-backend/index.ts"
  },
  "dependencies": {
    "@nitedani/shrink-ray-current": "^4.3.0",
    "@picocss/pico": "^2.0.6",
    "@react-router/node": "^7.1.3",
    "@react-router/serve": "^7.1.3",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "compression": "^1.7.5",
    "cursor-effects": "^1.0.17",
    "dotenv": "^16.4.7",
    "downshift": "^9.0.8",
    "express-async-handler": "^1.2.0",
    "express-formidable": "^1.2.0",
    "formidable": "^3.5.2",
    "http-proxy-middleware": "^3.0.3",
    "isbot": "^5.1.22",
    "js-confetti": "^0.12.0",
    "morgan": "^1.10.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-router": "^7.1.3",
    "rollbar": "^2.26.4",
    "sharp": "^0.33.5",
    "swr": "^2.3.2",
    "throttle-debounce": "^5.0.2",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@playwright/test": "^1.50.1",
    "@react-router/dev": "^7.1.3",
    "@types/formidable": "^3.4.5",
    "@types/node": "^22",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/throttle-debounce": "^5.0.2",
    "autoprefixer": "^10.4.20",
    "cheerio": "^1.0.0",
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "postcss": "^8.5.2",
    "sass-embedded": "^1.85.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.6"
  },
  "overrides": {
    "usehooks-ts": {
      "react": "^19"
    }
  }
}
