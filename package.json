{
  "name": "react-router-peterbecom",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build",
    "build:witholdbuildpreserved": "rm -fr /tmp/_old_build && tsx scripts/backup-old-build.ts && npm run build && tsx scripts/restore-old-build.ts",
    "dev": "react-router dev",
    "start": "cross-env NODE_ENV=production tsx start-server.ts ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepare": "husky",
    "test": "BUILD_PATH=./build/server/index.js vitest --run",
    "build:compress": "node --experimental-strip-types --no-warnings scripts/compress-build.ts",
    "compare-build-dirs": "node --experimental-strip-types --no-warnings scripts/compare-build-dirs.ts",
    "bot-agent-backfill": "tsx scripts/bot-agent-backfill.ts",
    "test:watch": "BUILD_PATH=./build/server/index.js vitest",
    "mock-peterbecom-backend": "tsx mock-peterbecom-backend/index.ts",
    "mock-peterbecom-backend:watch": "tsx watch mock-peterbecom-backend/index.ts",
    "test-user-agent": "tsx scripts/test-user-agent.ts",
    "postinstall": "npx @biomejs/biome migrate --write"
  },
  "dependencies": {
    "@picocss/pico": "^2.1.1",
    "@react-router/node": "^7.3.0",
    "@react-router/serve": "^7.3.0",
    "@types/compression": "^1.8.1",
    "@types/morgan": "^1.9.10",
    "axios": "^1.11.0",
    "axios-retry": "^4.5.0",
    "compression": "^1.8.1",
    "cursor-effects": "^1.0.17",
    "dotenv": "^17.2.2",
    "downshift": "^9.0.10",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^8.1.0",
    "formidable": "^3.5.4",
    "http-proxy-middleware": "^3.0.5",
    "isbot": "^5.1.31",
    "js-confetti": "^0.12.0",
    "morgan": "^1.10.1",
    "on-finished": "^2.4.1",
    "on-headers": "^1.0.2",
    "postgres": "^3.4.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-router": "^7.1.3",
    "rollbar": "^2.26.4",
    "sharp": "^0.34.4",
    "swr": "^2.3.6",
    "throttle-debounce": "^5.0.2",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@playwright/test": "^1.55.1",
    "@react-router/dev": "^7.6.3",
    "@types/formidable": "^3.4.5",
    "@types/node": "^24",
    "@types/on-finished": "^2.3.5",
    "@types/on-headers": "^1.0.4",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@types/throttle-debounce": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "cheerio": "^1.1.2",
    "cross-env": "^10.0.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "sass-embedded": "^1.92.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "usehooks-ts": {
      "react": "^19"
    }
  }
}
